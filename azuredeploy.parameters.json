{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceIndex": {
      "value": "00"
    },
    "vmUserName": {
      "value": "your-linux-admin-userid"
    },
    "vmPassword": {
      "value": "your-linux-admin-password"
    },
    "vhdStorageAccountName": {
      "value": "your-vhd-storage-account-name"
    },
    "spinnakerVMSize": {
      "value": "Standard_D2"
    },
    "jenkinsVMSize": {
      "value": "Standard_D2"
    },
    "spinnakerDnsPrefix": {
      "value": "your-dns-prefix"
    },
    "jenkinsDnsPrefix": {
      "value": "your-dns-prefix"
    },
    "spnSubscriptionId": {
      "value": "your-subscription-Id"
    },
    "spnClientId": {
      "value": "your-client-Id"
    },
    "spnClientSecret": {
      "value": "your-client-secret"
    },
    "spnTenantId": {
      "value": "your-tenant-Id"
    },
    "packerStorageAccountName": {
      "value": "your-packer-storage-account-name"
    },
    "packerResourceGroupName": {
      "value": "your-packer-resource-group-name"
    },
    "keyVaultAccountName": {
      "value": "your-keyvault-account-name"
    },
    "OracleUser" : {
      "value": "your-oracle-account-name"
    },
    "OraclePassword" : {
      "value" : "your-oracle-password"
    },
    "JenkinsUser" : {
      "value" : "your-jenkins-username"
    },
    "JenkinsPassword" : { 
      "value" : "your-jenkins-password"
    }
  }
}
